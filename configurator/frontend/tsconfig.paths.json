{
  //Any new alias should be added to craco.config.js as well
  "compilerOptions": {
    "paths": {
      "@./*": ["./*"],
      "@component/*": ["./ui/components/*"],
      "@page/*": ["./ui/pages/*"],
      "@catalog/*": ["./catalog/*"],
      "@service/*": ["./lib/services/*"],
      "@util/*": ["./utils/*"],
      "@hooks/*": ["./hooks/*"],
      "@hocs/*": ["./hocs/*"],
      "lodash/*": [
        "node_modules/@types/lodash-es/*"
      ]
    }
  }
}
